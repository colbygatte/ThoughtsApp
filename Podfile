# platform :ios, '9.0'

target 'cgPad' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for cgPad
  pod 'Firebase'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'FirebaseUI'
  pod 'DGElasticPullToRefresh'

  target 'cgPadTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase'
  end

  target 'cgPadUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
